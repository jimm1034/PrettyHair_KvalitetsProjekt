<Window x:Class="PrettyHair_KvalitetsProjekt.Content.Confirm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrettyHair_KvalitetsProjekt.Content"
        mc:Ignorable="d"
        Title="Confirm" Height="250" Width="300">
    <Grid Width="200" Height="200">
        <TextBlock Text="Bekræft" TextAlignment="Center" FontSize="30"/>
        <Button Name="confirmationTrue" Content="Ok" FontSize="20"
                IsDefault="True" Click="ConfirmationTrue_Click"
                Width="40" Height="40" HorizontalAlignment="Left" Margin="34,80,0,80"/>
        <Button Name="confirmationFalse" Content="Tilbage" FontSize="20"
                IsCancel="True" Click="ConfirmationFalse_Click"
                Width="83" Height="40" HorizontalAlignment="Right" Margin="0,80,24,80"/>
    </Grid>
</Window>
